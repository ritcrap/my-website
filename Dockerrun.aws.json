{
	"AWSEBDockerrunVersion": 2,
	"volumes": [
		{
			"name": "cachevolume",
			"host": {
				"sourcePath": "/var/api/cache"
			}
		}
	],
	"containerDefinitions": [
		{
			"name": "api",
			"image": "ritwik310/my-website-api",
			"hostname": "api",
			"essential": false,
			"memory": 128,
			"mountPoints": [
				{
					"sourceVolume": "cachevolume",
					"containerPath": "/usr/src/cache",
					"readOnly": false
				}
			]
		},
		{
			"name": "renderer",
			"image": "ritwik310/my-website-renderer",
			"hostname": "renderer",
			"essential": false,
			"links": [ "api" ],
			"memory": 128
		},
		{
			"name": "console",
			"image": "ritwik310/my-website-console",
			"hostname": "console",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "my-website-nginx",
			"hostname": "nginx",
			"essential": true,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": [ "api", "renderer", "console" ],
			"memory": 128
		}
	]
}
